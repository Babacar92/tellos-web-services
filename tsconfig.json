{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "target": "es2017",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": "./",
        "incremental": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
        "noImplicitAny": false,
        "strictBindCallApply": false,
        "forceConsistentCasingInFileNames": false,
        "noFallthroughCasesInSwitch": false,
        "paths": {
            "@/*": ["src/*"],
            "@Application/*": ["src/application/*"],
            "@Pipes/*": ["src/application/pipes/*"],
            "@Interceptors/*": ["src/application/interceptors/*"],
            "@Errors/*": ["src/application/errors/*"],
            "@Functions/*": ["src/application/functions/*"],
            "@Entities/*": ["src/entities/psql/*"],
            "@Types/*": ["src/types/*"],
            "@Validations/*": ["src/application/validations/*"],
            "@Auth/*": ["src/libs/auth/*"],
            "@Modules/*": ["src/modules/*"],
            "@ModuleEmployee/*": ["src/modules/employees/*"],
            "@ModuleContract/*": ["src/modules/contracts/*"],
            "@ModuleUser/*": ["src/modules/user/*"],
            "@ModuleSupplier/*": ["src/modules/group-supplier/*"],
            "@ModuleEquipment/*": ["src/modules/equipments/*"],
            "@Infrastructure/*": ["src/infrastructure/*"],
            "@DB_Postgres/*": ["src/infrastructure/database/postgres/*"],
            "@DB_Mongodb/*": ["src/infrastructure/database/mongodb/*"],
            "@Definitions/*": ["src/definitions/*"],
            "@Libs/*": ["src/libs/*"],
            "@Migrations/*": ["src/migrations/*"],
            "@Templates/*": ["src/templates/*"],
            "@Translations/*": ["src/templates/*"],
            "@Utils/*": ["src/utils/*"]
        }
    }
}
